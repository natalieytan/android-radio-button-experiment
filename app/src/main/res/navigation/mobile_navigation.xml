<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_unstyled_radio_button">

    <fragment
        android:id="@+id/nav_unstyled_radio_button"
        android:name="com.example.radiobuttonexperiment.ui.styledradiobutton.UnstyledRadioButtonFragment"
        android:label="@string/menu_unstyled_radio_button"
        tools:layout="@layout/fragment_unstyled_radio_button" />

    <fragment
        android:id="@+id/nav_styled_radio_button"
        android:name="com.example.radiobuttonexperiment.ui.styledradiobutton.StyledRadioButtonFragment"
        android:label="@string/menu_styled_radio_button"
        tools:layout="@layout/fragment_styled_radio_button" />

    <fragment
        android:id="@+id/nav_drawable_radio_button"
        android:name="com.example.radiobuttonexperiment.ui.drawableradiobutton.DrawableRadioButtonFragment"
        android:label="@string/menu_styled_drawable_radio_button"
        tools:layout="@layout/fragment_drawable_radio_button" />

    <fragment
        android:id="@+id/nav_custom_radio_button"
        android:name="com.example.radiobuttonexperiment.ui.customradiobutton.CustomRadioButtonFragment"
        android:label="@string/menu_custom_radio_button"
        tools:layout="@layout/fragment_custom_radio_button" />

    <fragment
        android:id="@+id/nav_custom_radio_group"
        android:name="com.example.radiobuttonexperiment.ui.customradiogroup.CustomRadioGroupFragment"
        android:label="@string/menu_custom_radio_group"
        tools:layout="@layout/fragment_custom_radio_group" />
</navigation>